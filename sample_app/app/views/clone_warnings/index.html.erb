

<h1>Warnings</h1>

<% if @clone_warnings && !@clone_warnings.empty? %> 

	<%= will_paginate %>

		<table border="1">
			<tr>
				<td>Percentagem de funções em que se detectou</td>
				<td>Ficheiro</td>
				<td>Comparado com</td>
				<td>ID concurso</td>
				<td>ID enunciado</td>
				<td>Eliminar warning</td>				
			</tr>
			
			<%= render @clone_warnings %>
				
		</table>

	<%= will_paginate %>

<% else %>

	Não há warnings.

<% end %>
